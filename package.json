{
  "name": "spraybottle",
  "version": "0.0.2",
  "description": "A cross platform tool for supercharging your scripts, and delivering them to end-users.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "spraybottle",
    "win": {
      "target": [
        "portable",
        "nsis"
      ]
    }
  },
  "repository": "https://github.com/jooshkins/spraybottle",
  "keywords": [
    "Electron",
    "scripts",
    "offline",
    "end user",
    "powershell"
  ],
  "author": "jooshkins",
  "license": "MIT",
  "devDependencies": {
    "@fortawesome/fontawesome-free-webfonts": "^1.0.6",
    "electron": "^1.8.4",
    "electron-builder": "^20.11.1",
    "electron-packager": "^12.0.1",
    "electron-titlebar-windows": "^3.0.0",
    "electron-winstaller": "^2.6.4",
    "executable": "^4.1.1",
    "node-powershell": "^3.3.1",
    "sudo-prompt": "^8.2.0",
    "electron-prebuilt-compile": "2.0.0",
    "npm": "^5.8.0"
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "electron-store": "^1.3.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.0",
    "powershell": "^2.3.0",
    "split.js": "^1.3.5"
  }
}